{
  "name": "nivo-label-example",
  "version": "0.0.0",
  "description": "Example fix for Nivo chart labels in Firefox",
  "main": "src/index.js",
  "scripts": {
    "dist": "browserify src/index.tsx -t browserify-css -p [tsify -p tsconfig.dist.json] -o dist/index.js",
    "start": "budo src/index.tsx:dist/index.js --live -- -t browserify-css -p [tsify -p tsconfig.dist.json]",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakesidsmith/nivo-label-example.git"
  },
  "keywords": [
    "nivo",
    "charts",
    "label",
    "fix",
    "firefox"
  ],
