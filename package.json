  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakesidsmith/nivo-label-example/issues"
  },
  "homepage": "https://github.com/jakesidsmith/nivo-label-example#readme",
  "dependencies": {
    "@nivo/bar": "^0.55.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "browserify": "^16.2.3",
    "browserify-css": "^0.14.0",
    "core-js": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsify": "^4.0.1",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "budo": "^11.6.1"
  }
